#!/bin/bash

enter_git_repository https://github.com/troydhanson/uthash.git "v$version"

PKG_NAME=uthash

cat << EOF > $PKG_NAME.pc
Name: $PKG_NAME
Description: C macros for hash tables and more 
URL: github.com/troydhanson/uthash
Requires: 
Version: $version
Libs: 
Cflags: -I$WIN_INSTALL_PREFIX/include
EOF

install_pkgconfig_file "$PKG_NAME.pc"

